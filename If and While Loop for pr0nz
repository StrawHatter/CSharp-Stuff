using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Whelp
{
    class Program
    {
        static void Main()
        {
            int i = 0, input, no_people;
            string inputString, inpuno_people;

            inpuno_people = Console.ReadLine();
            no_people = int.Parse(inpuno_people); // Inputing stuff in C# is fun since you input as string then convert to int using console.parse

            while (i < no_people)  // C# equiv of  For Next loop 
            {
                inputString = Console.ReadLine();
                input = int.Parse(inputString);

                if (input < 18 && input > 0)  // if inputed age is < 18 && makes sure it's a valid age, responds in kinda
                    Console.WriteLine("Get off this thing");
                else
                    Console.WriteLine("Enjoy your visit!");

                i = i + 1; // incrament counter of i by 1 each time loop finishes
            }

        }
    }
}
